[Unit]
Description=CertWarden Certificate Deployment
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
User=root
Group=root
ExecStart=/usr/local/bin/certwarden-deploy.py process --config /etc/certwarden-deploy/config.yaml
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target